<%= stylesheet_link_tag "users", :media => "all" %>
<% content_for :title do %>
    パスワード変更
<% end %>
<%= form_with scope: :user, url: update_password_path(@user), method: :post, class: "form-password"  do |f| %>
  <%= f.password_field :password,  class: 'form-control', :placeholder => "パスワード" %>
  <%= f.password_field :password_confirmation,  class: 'form-control', :placeholder => "パスワード(確認)" %>
  <%= f.submit '登録', class: 'btn btn-lg btn-primary btn-block' %>
<% end %>